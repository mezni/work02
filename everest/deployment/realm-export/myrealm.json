{
  "realm": "myrealm",
  "enabled": true,

  "roles": {
    "realm": [
      { "name": "admin" },
      { "name": "user" },
      { "name": "partner" },
      { "name": "operator" }
    ],
    "client": {
      "my-api": [
        { "name": "api_reader" },
        { "name": "api_writer" }
      ]
    }
  },

  "groups": [
    { "name": "managers" }
  ],

  "clients": [
    {
      "clientId": "my-api",
      "name": "My API",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "my-secret-key",
      "redirectUris": ["http://localhost/*"],
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true
    }
  ],

  "users": [
    {
      "username": "john",
      "enabled": true,
      "emailVerified": true,
      "email": "john@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "test123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user"
      ],
      "groups": [
        "managers"
      ]
    }
  ]
}