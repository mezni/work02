{
  "realm": {
    "realm": "ev-realm",
    "enabled": true,
    "roles": {
      "realm": [
        {
          "name": "admin"
        },
        {
          "name": "operator"
        },
        {
          "name": "partner"
        },
        {
          "name": "user"
        },
        {
          "name": "guest"
        }
      ]
    },
    "clients": [
      {
        "clientId": "auth-service",
        "enabled": true,
        "directAccessGrantsEnabled": true,
        "publicClient": false,
        "protocol": "openid-connect",
        "defaultRoles": ["user"],
        "serviceAccountsEnabled": true,
        "clientRoles": {
          "realm-management": [
            "manage-users",
            "view-users",
            "view-realm"
          ]
        }
      }
    ],
    "users": [
      {
        "username": "admin",
        "email": "admin@example.com",
        "enabled": true,
        "credentials": [
          {
            "type": "password",
            "value": "password"
          }
        ],
        "realmRoles": [
          "admin"
        ],
        "clientRoles": {
          "auth-service": [
            "admin"
          ]
        }
      }
    ]
  }
}