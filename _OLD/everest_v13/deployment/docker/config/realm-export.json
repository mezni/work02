{
  "realm": "auth-service-realm",
  "enabled": true,
  "displayName": "Auth Service Realm",
  "registrationAllowed": false,
  "resetPasswordAllowed": true,
  "rememberMe": true,
  "verifyEmail": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "sslRequired": "external",
  "accessTokenLifespan": 300,
  "accessTokenLifespanForImplicitFlow": 900,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "offlineSessionIdleTimeout": 2592000,
  "accessCodeLifespan": 60,
  "accessCodeLifespanUserAction": 300,
  "accessCodeLifespanLogin": 1800,
  "clients": [
    {
      "clientId": "auth-service",
      "name": "Auth Service Client",
      "description": "Client for managing users and authentication",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "your-client-secret-here-change-me",
      "redirectUris": [
        "http://localhost:5080/*",
        "https://your-domain.com/*"
      ],
      "webOrigins": [
        "http://localhost:5080",
        "https://your-domain.com"
      ],
      "protocol": "openid-connect",
      "publicClient": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true,
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "fullScopeAllowed": true,
      "attributes": {
        "access.token.lifespan": "300"
      }
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user-manager",
        "description": "Can manage users"
      },
      {
        "name": "permission-manager",
        "description": "Can manage permissions"
      },
      {
        "name": "auth-admin",
        "description": "Full authentication administration"
      }
    ],
    "client": {
      "auth-service": [
        {
          "name": "manage-users",
          "description": "Manage user accounts"
        },
        {
          "name": "manage-permissions",
          "description": "Manage user permissions"
        },
        {
          "name": "manage-auth",
          "description": "Manage authentication"
        }
      ]
    }
  },
  "users": [
    {
      "username": "service-account-auth-service",
      "enabled": true,
      "serviceAccountClientId": "auth-service",
      "realmRoles": [
        "user-manager",
        "permission-manager",
        "auth-admin"
      ],
      "clientRoles": {
        "realm-management": [
          "manage-users",
          "view-users",
          "query-users",
          "manage-clients",
          "view-clients"
        ]
      }
    }
  ],
  "defaultRoles": [
    "offline_access",
    "uma_authorization"
  ]
}