{
  "realm": "myrealm",
  "enabled": true,
  "roles": {
    "realm": [
      {
        "name": "admin",
        "description": "Administrator role with full access"
      },
      {
        "name": "user",
        "description": "Regular user role"
      },
      {
        "name": "operator",
        "description": "Operator role for system operations"
      },
      {
        "name": "partner",
        "description": "Partner/external collaborator role"
      }
    ]
  },
  "clients": [
    {
      "clientId": "authclient",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "redirectUris": ["http://localhost:8081/callback"],
      "webOrigins": ["http://localhost:8081"],
      "protocolMappers": [
        {
          "name": "company_name",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "company_name",
            "access.token.claim": "company_name",
            "claim.name": "company_name",
            "jsonType.label": "String"
          }
        },
        {
          "name": "station_name",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "station_name",
            "access.token.claim": "station_name",
            "claim.name": "station_name",
            "jsonType.label": "String"
          }
        }
      ]
    }
  ]
}